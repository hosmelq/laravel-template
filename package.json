    "@types/node": "^24.0.10",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.8.1",
    "husky": "^9.1.7",
    "laravel-vite-plugin": "^2.0.0",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.1.5",
    "oxlint": "^1.14.0",
    "prettier": "^3.6.2",
