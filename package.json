{
  "name": "template",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "tsgo -b && vite build",
    "prebuild": "composer wayfinder",
    "dev": "vite",
    "fix": "oxlint --fix --fix-dangerously --fix-suggestions --tsconfig tsconfig.json --type-aware",
    "fmt": "bun run fmt:oxfmt && bun run fmt:prettier",
    "fmt:oxfmt": "oxfmt",
    "fmt:prettier": "prettier --write \"**/*.{css,json}\"",
    "lint": "oxlint --tsconfig tsconfig.json --type-aware",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroui/react": "^3.0.0-beta.6",
    "@heroui/styles": "^3.0.0-beta.6",
    "@inertiajs/react": "^2.3.15",
    "@inertiaui/modal-react": "1.0.0-beta-5",
    "@sentry/react": "^10.39.0",
    "react": "^19.2.4",
    "react-aria-components": "^1.15.1",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@laravel/vite-plugin-wayfinder": "^0.1.7",
    "@sentry/vite-plugin": "^4.9.1",
    "@tailwindcss/vite": "^4.2.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260218.1",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "laravel-vite-plugin": "^2.1.0",
    "lefthook": "^2.1.1",
    "lightningcss": "^1.31.1",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "oxlint-tsgolint": "^0.14.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.2.0",
    "tailwindcss-react-aria-components": "^2.0.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@^7.3.1",
    "vite-plugin-manifest-sri": "^0.2.0",
    "vite-tsconfig-paths": "^6.1.1"
  }
}
